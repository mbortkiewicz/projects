	}
	
	public TPozycja nowaPozycja(Object[] data)
	{
		TPozycja pozycja = new TPozycja();
		
		pozycja.setProdukt((TProdukt) data[0]);
		pozycja.setStawka((float) data[1]);
		pozycja.setOddane((boolean) data[2]);
		
		return pozycja;
	}
}
